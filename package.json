    "prettier": "^2.6.2",
    "prettyjson": "^1.2.5",
    "read-cache": "^1.0.0",
    "resolve": "^1.22.0",
    "sass": "^1.51.0",
    "stylelint": "^14.8.0",
    "stylelint-config-standard": "^24.0.0",
    "tailwindcss": "^3.0.24",
    "urlcat": "^2.0.4"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.10.2",
    "alpinejs": "^3.10.2",
    "choices.js": "^10.1.0",
    "flickity": "^3.0.0",
    "flickity-as-nav-for": "^3.0.0",
    "gsap": "^3.10.4",
    "mixitup": "^3.3.1"
  }

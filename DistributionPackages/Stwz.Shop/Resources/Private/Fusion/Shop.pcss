.addressLookup {
    
    
    input,
    .button {
        @apply !py-2;
        @apply font-extrabold;
    }

    input {
        @apply rounded-full h-10 bg-transparent border placeholder-black placeholder-opacity-100 focus:outline-none focus:ring-1 focus:ring-white focus:border-white;
    }
    
    .button {
        @apply font-sans h-10 rounded-full bg-white px-6 py-2 hover:scale-105 duration-150;
    }

}
